        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.prayercircle.prayercircle"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": "ba379de7-859d-424a-a1be-46c133aab81a"
      }
    },
    "owner": "prayer-circle"
  }
